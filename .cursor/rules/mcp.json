{
    "mcpServers": {
      "serverless": {
        "command": "serverless",
        "args": ["mcp"]
      },
      "awslabs.aws-serverless-mcp-server": {
        "command": "uvx",
        "args": ["awslabs.aws-serverless-mcp-server@latest", "--allow-write", "--allow-sensitive-data-access"],
        "env": { "AWS_PROFILE": "...", "AWS_REGION": "..." }
      }
    }
  }
  